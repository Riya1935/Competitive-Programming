// Question Link: https://codeforces.com/contest/1328/problem/A

// My solution

#include <bits/stdc++.h>
using namespace std;
int main()
   {
   	  int t;
   	  cin>>t;
   	  while(t--)
   	     {
   	     	int a,b;
   	     	cin>>a>>b;
   	     	int x=a%b;
   	     	if(x==0)  cout<<"0"<<endl;
   	     	else
   	     	cout<<b-x<<endl;
   	     }
   } 
